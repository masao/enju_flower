<%- case @attachable.class.to_s when 'Shelf' -%>
  <%= render :partial => 'index_shelf', :locals => {:shelf => @attachable} -%>
<%- when 'Manifestation' -%>
  <%= render :partial => 'index_manifestation', :locals => {:manifestation => @attachable} -%>
<%- when 'Patron' -%>
  <%= render :partial => 'index_patron', :locals => {:patron => @attachable} -%>
<%- when 'Event' -%>
  <%= render :partial => 'index_event', :locals => {:event => @attachable} -%>
<%- else -%>
  <%= render :partial => 'index' -%>
<%- end -%>
