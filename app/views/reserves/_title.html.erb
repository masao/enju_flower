    <strong><%= link_to reserve.manifestation.original_title, reserve -%></strong>
    <%- if reserve.manifestation.serial? -%>
      <%= reserve.manifestation.volume_number_list -%>
      <%- unless reserve.manifestation.issue_number_list.blank? -%>
        (<%= reserve.manifestation.issue_number_list -%>)
      <%- end -%>
      <%= reserve.manifestation.serial_number_list -%>
    <%- end -%>
    <br />
    <%- if reserve.manifestation.creators.readable_by(current_user).exists? -%>
      <%= t('patron.creator') -%>: <%= raw patrons_list(reserve.manifestation.creators.readable_by(current_user)) -%>
    <%- end -%>
    <%- if reserve.manifestation.contributors.readable_by(current_user).exists? -%>
      <%= t('patron.contributor') -%>: <%= raw patrons_list(reserve.manifestation.contributors.readable_by(current_user)) -%>
    <%- end -%>
    <%- if reserve.manifestation.publishers.readable_by(current_user).exists? -%>
      <%= t('patron.publisher') -%>: <%= raw patrons_list(reserve.manifestation.publishers.readable_by(current_user)) -%>
      <%- if reserve.manifestation.date_of_publication -%>
        (<%= reserve.manifestation.date_of_publication.strftime('%Y') -%>)
      <%- end -%>
    <%- end -%>
    <%- unless reserve.manifestation.access_address.blank? -%>
      <br />
      <%= link_to reserve.manifestation.access_address, manifestation.access_address -%>
      <%= link_to image_tag('icons/world_go.png', :size => '16x16', :alt => ('Web')), reserve.manifestation.access_address -%>
    <%- end -%>
